{
    "name": "kishor-rajbanshi/php-crypt",
    "description": "The PHP Encryptor and Decryptor",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/kishor-rajbanshi/php-crypt",
    "keywords": [
        "encryption",
        "openssl",
        "aes",
        "crypt",
        "security"
    ],
    "support": {
        "issues": "https://github.com/kishor-rajbanshi/php-crypt/issues",
        "source": "https://github.com/kishor-rajbanshi/php-crypt"
    },
    "authors": [
        {
            "name": "Kishor Rajbanshi",
            "email": "kishorrajbanshi00@gmail.com",
            "homepage": "https://kishor-rajbanshi.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "KishorRajbanshi\\Crypt\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^4.3"
    },
    "scripts": {
        "test": "pest"
    }
}